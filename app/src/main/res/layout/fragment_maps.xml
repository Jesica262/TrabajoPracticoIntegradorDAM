<?xml version="1.0" encoding="utf-8"?>


   <fragment xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/map"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:name="com.google.android.gms.maps.SupportMapFragment"
    tools:context=".FragmentPedirArbol"
    >
   <com.google.android.material.floatingactionbutton.FloatingActionButton
       android:id="@+id/floatingAdd"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:src="@drawable/baseline_add_24"
       android:layout_marginLeft="50pt"
       android:layout_marginBottom="50pt"
       android:layout_gravity="left|bottom"
       android:contentDescription="@string/agregar"
       android:layout_alignParentBottom="true"
       android:layout_alignLeft="@+id/floatingCheck"
       android:layout_alignStart="@+id/floatingCheck"
       android:layout_margin="40dp"
       />

   <com.google.android.material.floatingactionbutton.FloatingActionButton
       android:id="@+id/floatingCheck"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:src="@drawable/ic_baseline_check_24"
       android:layout_above="@+id/floatingAdd"
       android:layout_alignParentLeft="true"
       android:layout_alignParentStart="true"
       android:layout_gravity="end|bottom"
       android:contentDescription="@string/AceptarArbol"
       android:layout_margin="40dp"
 />



</fragment>





